    "knip": "knip",
    "lint": "eslint",
    "prepare": "husky",
    "start": "next start --port 1337",
    "start:e2e": "next start --port 1337",
    "test": "vitest run",
    "test:e2e": "playwright test",
    "test:watch": "vitest",
    "update": "tsx ./deploy.ts ./pact/deploy/update.json",
    "update:del": "tsx ./deploy.ts ./pact/deploy/local-delivery.json",
    "update:dev": "tsx ./deploy.ts ./pact/deploy/local-update.json",
    "update:hosted": "tsx ./deploy.ts ./pact/deploy/hosted-update.json",
    "update:mainnet": "tsx ./deploy.ts ./pact/deploy/mainnet-update.json",
    "update:test": "tsx ./deploy.ts ./pact/deploy/testnet-update.json"
